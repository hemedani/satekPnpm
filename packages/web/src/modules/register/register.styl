  .pageStore
    min-height 100vh
    display flex
    background-image url(../image/backgroundRegister/docter.png)
    background-repeat no-repeat
    background-size cover
    background-position center
    box-sizing border-box
  .registerStore
    flex 7
    max-width: 66rem;
    display flex
    margin-right 25%
    @media only screen and (max-width: 850px)  
      margin: 0
    
  .registerStoreBackground
    flex 3
  .registerStoreRight
    flex 2
    @media only screen and (max-width: 975px)  
      display none
  