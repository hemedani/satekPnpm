
.containerRegisterPhoto
  display flex
  align-items center
  direction rtl
  width 100%
  // overflow-y: auto
  flex-direction column
  
[type="file"]  
  height: 0;
  overflow: hidden;
  width: 0;
.label-file-select
  padding: 0.5rem 0.8rem
.containerRegisterPhotoOwner
  display flex
  align-items center
  direction rtl
  width 100%
  flex-direction column
.boxTextRegisterPhotoEdit
  display flex
.itemRegisterPhotoEdit
  display flex
  width 90%
  border-bottom 1px solid gray 
  justify-content space-between
  align-items center
  flex 1 1
  padding: 1.5rem 0 
.part-itemRegisterPhotoEdit-input
  display flex  
  justify-content space-between
  align-items center
  flex 1 1
  width 100%
.title-itemRegisterPhotoEdit
  font-family: FontMedium
  font-size: 0.8rem
  white-space nowrap
  padding-left: 0.5rem
.box-input-itemRegisterPhotoEdit
  align-items center 
  display: flex
  width: 90%;
  margin-top: 0.5rem
.itemRegisterPhotoEdit-input
  display flex
  width 90%
  flex-direction: column
  border-bottom 1px solid gray 
  justify-content space-between
  align-items center
  padding: 1.5rem 0
  flex 1 1
.itemRegisterPhotoEdit-width
  width 45%
  padding: 0.5rem
  flex: unset
  margin: 0.3rem 0
  flex-direction: column;
  min-height: 13.5rem;
  border-radius: 0.6rem
  border: dashed 0.08rem black
  @media only screen and (max-width: 665px)  
    width 90%
.textExplainRegisterPhotoEdit
  font-size 0.6rem
  margin 2px 2em 0 0 
  font-family: FontMedium
.textRegisterPhotoEdit
  margin 0
  font-family: FontMedium
.input-date-itemRegisterPhotoEdit 
  display: flex
  width 100%
  align-items: center
  border 1px solid gray
  box-shadow: unset
  justify-content: center
.responsive-calendar-itemRegisterPhotoEdit
  font-size: 7px
.itemDownRegisterEdit
  display flex
  width 90%
  justify-content space-between
  align-items center
  flex 1 1 5em
.itemDownRegisterPhotoEdit
  display flex
  width 90%
  justify-content space-between
  align-items center
  flex 1 1 5em
  margin-bottom 2em
.itemAddMemberDownRegisterPhotoEdit
  display flex
  width 90%
  justify-content space-between
  margin-bottom 2em
  align-items center
  flex 1 1 3em  
.img-containerRegisterPhoto
  height 4rem
  width 4rem
  margin 0.5rem 0 0 0 
.img-size-containerRegisterPhoto
  padding: 0
  margin: 0.5rem 0 0 0 
  height :6rem
  width :6rem
.textAddMemberDownRegisterPhotoEdit
  color blue
.img-uploader
  display: flex
  justify-content: center
.text-upload-center
  text-align: center
  line-height: 170%
.up-itemRegisterPhotoEdit
  justify-content: space-around
  display: flex
  @media only screen and (max-width: 665px)  
    flex-direction: column
    align-items: center
.border-itemRegisterPhotoEdit
  display: flex
  flex-direction: column
  cursor pointer
  flex: 1
  font-family: FontMedium
.border-new-itemRegisterPhotoEdit
  border: 1px solid black
.selectBtnFileRegisterPhotoEdit
  background-color blue
  border-radius 5px
  border none
  color white
  height 2em
